{
  "name": "taskbox",
  "version": "0.1.0",
  "description": "Starter template to get up and running quickly with Angular and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-angular-template"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-angular-template/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --watchAll",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && storybook dev -p 6006",
    "build-storybook": "npm run docs:json && storybook build",
    "init-msw": "msw init src/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.1.3",
    "@angular/common": "~15.1.3",
    "@angular/compiler": "~15.1.3",
    "@angular/core": "~15.1.3",
    "@angular/forms": "~15.1.3",
    "@angular/platform-browser": "~15.1.3",
    "@angular/platform-browser-dynamic": "~15.1.3",
    "@angular/router": "~15.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.1.4",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "~15.1.3",
    "@babel/core": "^7.18.2",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "7.0.0-beta.43",
    "@storybook/addon-essentials": "7.0.0-beta.43",
    "@storybook/addon-interactions": "7.0.0-beta.43",
    "@storybook/addon-links": "7.0.0-beta.43",
    "@storybook/angular": "7.0.0-beta.43",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.9",
    "babel-loader": "^9.1.2",
    "jest": "^29.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.43",
    "typescript": "~4.9.5"
  }
}
